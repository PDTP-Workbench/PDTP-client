{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": false,
		"lib": ["DOM", "ESNext", "DOM.Iterable"]
	},
	"exclude": ["vite.config.ts", "dist"]
}
